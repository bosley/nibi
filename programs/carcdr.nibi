(fn car [x] (clone (at x 0)))
(fn cdr [x] (<<| (clone x)))
(fn cadr [x] (? (>= (len x) 2)
                  (<- (clone (at x 1)))
                  (<- [])))

(:= l [99 2 3 4 5 6])

# Get the first element
(assert (eq 99 (car l)) "nope")

# Get all but first 
(assert (eq "[2 3 4 5 6]" (cdr l)) "nope")

# Get the second element
(assert (eq 2 (cadr l)) "nope")

